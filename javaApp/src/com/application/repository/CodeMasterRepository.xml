      </if>
      <if test="linkUrl!=null and linkUrl!=''">
        AND LINK_URL = #{linkUrl}
      </if>
      <if test="imgUrl!=null and imgUrl!=''">
        AND IMG_URL = #{imgUrl}
      </if>
      <if test="roleGroup!=null and roleGroup!=''">
        AND ROLE_GROUP = #{roleGroup}
      </if>
      <if test="parentId!=null and parentId!=''">

      <if test="memo2!=null and memo2!=''">
        MEMO2 = #{memo2},
      </if>
      <if test="memo3!=null and memo3!=''">
        MEMO3 = #{memo3},
      </if>
      <if test="numberCol1!=null and numberCol1!=''">

  <sql id="select_column_list">
    `ID`
    ,`APPLICATION`
    ,`CATEGORY_ID`
    ,`CATEGORY_NAME`
    ,`CODE`
    ,`NAME`
    ,`LINK_URL`
    ,`IMG_URL`
    ,`ROLE_GROUP`
    ,`PARENT_ID`
    ,`MEMO1`
    ,`MEMO2`
    ,`MEMO3`
    ,`NUMBER_COL1`
    ,`NUMBER_COL2`
    ,`NUMBER_COL3`
  </sql>

  <sql id="insert_column_list">
    `ID`
    ,`APPLICATION`
    ,`CATEGORY_ID`
    ,`CATEGORY_NAME`
    ,`CODE`
    ,`NAME`
    ,`LINK_URL`
    ,`IMG_URL`
    ,`ROLE_GROUP`

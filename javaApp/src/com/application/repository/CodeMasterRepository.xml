      <if test="numberCol2!=null and numberCol2!=''">
        AND NUMBER_COL2 = #{numberCol2}
      </if>
      <if test="numberCol3!=null and numberCol3!=''">
        AND NUMBER_COL3 = #{numberCol3}
      </if>
      <if test="condition!=null and condition!=''">
        ${condition}
      </if>
    </where>
  </select>


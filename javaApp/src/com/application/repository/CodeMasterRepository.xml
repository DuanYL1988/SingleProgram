<!DOCTYPE mapper PUBLIC "-//MYBATIS.org//DTD Mapper 3.0//EN" "http://MYBATIS.org/dtd/MYBATIS-3-mapper.dtd" >
<mapper namespace="com.application.repository.CodeMasterRepository" >
  <!-- ResultMap -->
  <resultMap id="CodeMasterResultMap" type="com.application.model.CodeMaster" >
